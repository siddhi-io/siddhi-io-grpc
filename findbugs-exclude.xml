<FindBugsFilter>
    <Match>
        <Class name="org.wso2.grpc.Event$Builder"/>
        <Bug pattern="UCF_USELESS_CONTROL_FLOW"/>
    </Match>
    <Match>
        <Class name="org.wso2.grpc.Event$Builder"/>
        <Bug pattern="DLS_DEAD_LOCAL_STORE"/>
    </Match>
    <Match>
        <Class name="org.wso2.grpc.Event"/>
        <Bug pattern="SE_BAD_FIELD"/>
    </Match>
    <Match>
        <Class name="io.siddhi.extension.io.grpc.sink.GrpcCallSink$1"/>
        <Bug pattern="NP_PARAMETER_MUST_BE_NONNULL_BUT_MARKED_AS_NULLABLE"/>
    </Match>
    <Match>
        <Class name="io.siddhi.extension.io.grpc.source.GrpcEventServiceServer"/>
        <Bug pattern="MS_SHOULD_BE_FINAL, PATH_TRAVERSAL_IN, IMPROPER_UNICODE"/>
    </Match>
    <Match>
        <Class name="io.siddhi.extension.io.grpc.source.GenericServiceServer"/>
        <Bug pattern="MS_SHOULD_BE_FINAL, PATH_TRAVERSAL_IN, SIC_INNER_SHOULD_BE_STATIC_ANON"/>
    </Match>
    <Match>
        <Class name="io.siddhi.extension.io.grpc.source.AbstractGrpcSource"/>
        <Bug pattern="DM_CONVERT_CASE"/>
    </Match>
    <Match>
        <Class name="io.siddhi.extension.io.grpc.sink.GrpcCallSink$ListenableFutureWrapper$1"/>
        <Bug pattern="BC_UNCONFIRMED_CAST"/>
    </Match>
    <Match>
        <Class name="io.siddhi.extension.io.grpc.sink.GrpcCallSink$ListenableFutureWrapper$2"/>
        <Bug pattern="BC_UNCONFIRMED_CAST"/>
    </Match>
    <Match>
        <Class name="io.siddhi.extension.io.grpc.sink.GrpcCallSink$ListenableFutureWrapper$1"/>
        <Bug pattern="NP_PARAMETER_MUST_BE_NONNULL_BUT_MARKED_AS_NULLABLE"/>
    </Match>
    <Match>
        <Class name="io.siddhi.extension.io.grpc.source.GenericServiceServer$1"/>
        <Bug pattern="SIC_INNER_SHOULD_BE_STATIC_ANON"/>
    </Match>
    <Match>
        <Class name="io.siddhi.extension.io.grpc.sink.AbstractGrpcSink" />
        <Bug pattern="PATH_TRAVERSAL_IN" />
    </Match>
    <Match>
        <Class name="io.siddhi.extension.io.grpc.sink.GrpcCallSink" />
        <Bug pattern="IMPROPER_UNICODE" />
    </Match>
    <Match>
        <Class name="io.siddhi.extension.io.grpc.sink.GrpcServiceResponseSink" />
        <Bug pattern="IMPROPER_UNICODE" />
    </Match>
    <Match>
        <Class name="io.siddhi.extension.io.grpc.sink.GrpcSink" />
        <Bug pattern="IMPROPER_UNICODE" />
    </Match>
    <Match>
        <Class name="io.siddhi.extension.io.grpc.util.ServiceConfigs" />
        <Bug pattern="POTENTIAL_XML_INJECTION, CRLF_INJECTION_LOGS, IMPROPER_UNICODE" />
    </Match>
</FindBugsFilter>
